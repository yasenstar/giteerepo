    <!-- http://www.semanticweb.org/v0cn037/ontologies/2023/9/chemistry#01.01.02_分散系及其分类 -->

    <owl:Class rdf:about="http://www.semanticweb.org/v0cn037/ontologies/2023/9/chemistry#01.01.02_分散系及其分类">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/v0cn037/ontologies/2023/9/chemistry#01.01_物质的分类及转化"/>
        <chemistry:定义>把一种（或多种）物质以粒子形式分散到另一种（或多种）物质中所形成的混合物，叫做分散系。
分散系中被分散成粒子的物质叫分散质，另一种物质叫分散剂。</chemistry:定义>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/v0cn037/ontologies/2023/9/chemistry#01.01.02.01_溶液 -->

    <owl:Class rdf:about="http://www.semanticweb.org/v0cn037/ontologies/2023/9/chemistry#01.01.02.01_溶液">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/v0cn037/ontologies/2023/9/chemistry#01.01.02_分散系及其分类"/>
        <chemistry:定义>分散质粒子的直径小于1nm的是溶液。</chemistry:定义>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/v0cn037/ontologies/2023/9/chemistry#01.01.02.02_胶体 -->

    <owl:Class rdf:about="http://www.semanticweb.org/v0cn037/ontologies/2023/9/chemistry#01.01.02.02_胶体">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/v0cn037/ontologies/2023/9/chemistry#01.01.02_分散系及其分类"/>
        <chemistry:定义>分散质粒子的直径为1~100nm的是胶体</chemistry:定义>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/v0cn037/ontologies/2023/9/chemistry#01.01.02.03_乳浊液或悬浊液 -->

